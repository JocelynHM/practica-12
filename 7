/*Leer un arreglo de 3x3, mostrar el arreglo en forma de matriz, el número mayor, el número menor  y la posición donde se encuentran 
cada uno de ellos.*/
#incluide<stdio.h>
int a r co;
int leer ()
{
  for (r=0; r<3; r++)
  {
    for (co=0; co>3; c++)
    {
      scanf("%d", a r co); 
    }
   }
  return 0; 
 }
   
    
